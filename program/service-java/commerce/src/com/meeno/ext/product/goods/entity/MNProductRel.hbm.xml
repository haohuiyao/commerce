<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.meeno.ext.product.goods.entity">
	<class name="MNProductRel" table="mn_product_rel">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="native">
			</generator>
		</id>
		<property name="relType" type="java.lang.Integer" column="rel_type" />
		<property name="objType1" type="java.lang.Integer" column="obj_type1" />
		<property name="objId1" type="java.lang.Long" column="obj_id1" />
		<property name="objType2" type="java.lang.Integer" column="obj_type2" />
		<property name="objId2" type="java.lang.Long" column="obj_id2" />
		<property name="tag" type="java.lang.Integer" column="tag" />
		<property name="createTime" type="java.util.Date" column="create_time" />
	</class>
</hibernate-mapping>
