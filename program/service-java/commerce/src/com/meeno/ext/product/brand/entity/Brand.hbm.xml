<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.meeno.ext.product.brand.entity">
	<class name="Brand" table="mn_product_brand">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="native">
	        </generator>
		</id>
	
		<property name="name" type="java.lang.String" column="BRAND_NAME"/>
		<property name="abbreviation" type="java.lang.String" column="ABBREVIATION"/>
		<property name="imgUrl" type="java.lang.String" column="IMG_URL"/>
		<property name="status" type="java.lang.String" column="STATUS"/>
		<property name="groom" type="java.lang.String" column="GROOM"/>
		<property name="groomTime" type="java.sql.Date" column="GROOM_TIME"/>
	</class>
</hibernate-mapping>
