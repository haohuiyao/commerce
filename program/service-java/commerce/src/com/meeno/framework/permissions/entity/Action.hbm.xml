<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.meeno.framework.permissions.entity">
	<class name="Action" table="mnu_action">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="native">
	        </generator>
		</id>
	
		<property name="parentId" type="java.lang.Long" column="PARENT_ID" />
		<property name="title" type="java.lang.String" column="title" />
		<property name="href" type="java.lang.String" column="href" />
		<property name="icon" type="java.lang.String" column="ICON_URL" />
	</class>
</hibernate-mapping>
